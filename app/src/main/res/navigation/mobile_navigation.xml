<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.mobilebreakero.netclan.ui.screens.explore.ExploreFragment"
        tools:layout="@layout/fragment_explore" />

    <fragment
        android:id="@+id/navigation_network"
        android:name="com.mobilebreakero.netclan.ui.screens.network.NetworkFragment"
        tools:layout="@layout/fragment_network" />

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.mobilebreakero.netclan.ui.screens.chat.ChatFragment"
        tools:layout="@layout/fragment_chat" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.mobilebreakero.netclan.ui.screens.contacts.ProfileFragment"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_services"
        android:name="com.mobilebreakero.netclan.ui.screens.services.ServicesFragment"
        />

</navigation>